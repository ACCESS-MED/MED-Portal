@article{<%= model.bibtex_key %>,
  doi = {<%= model.doi %>},
  url = {https://doi.org/<%= model.doi %>},
  year = {<%= model.year %>},
  publisher = {The Open Journal},
  volume = {<%= model.volume %>},
  number = {<%= model.issue %>},
  pages = {<%= model.page %>},
  author = {<%= model.bibtex_authors %>},
  title = {<%= model.scholar_title %>},
  journal = {<%= setting(:name) %>}
}
